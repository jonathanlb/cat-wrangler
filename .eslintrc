{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": [ "airbnb", "plugin:import/warnings", "plugin:jest/recommended" ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [ "jest" ],
  "rules": {
    dot-location: [2, "object"],
    jest/valid-expect: 0,
    operator-linebreak: ["error", "after"]
  },
}
